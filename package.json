{
  "author": "Titux Metal (DEV) <github@lgdweb.fr>",
  "description": "Nest Expenses Api from Code With Vlad course",
  "name": "20230128-nest-vlad-expenses-api",
  "license": "MIT",
  "private": true,
  "repository": "https://github.com/TituxMetal/nest-vlad-expenses-api",
  "version": "1.0.0",
  "scripts": {
    "build": "nest build",
    "docker:up": "docker compose --project-name nest-vlad-expenses-api --file ./docker/docker-compose.yaml up -d",
    "docker:down": "docker compose --project-name nest-vlad-expenses-api --file ./docker/docker-compose.yaml down",
    "prisma": "prisma",
    "prisma:dev:deploy": "prisma migrate deploy",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch --runInBand --no-cache",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "9.4.3",
    "@nestjs/config": "3.2.0",
    "@nestjs/core": "9.4.3",
    "@nestjs/platform-express": "9.4.3",
    "@nestjs/schedule": "4.0.1",
    "@prisma/client": "4.16.2",
    "argon2": "0.31.2",
    "cache-manager": "5.4.0",
    "cache-manager-redis-store": "2.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "connect-redis": "7.1.1",
    "express-session": "1.18.0",
    "redis": "4.6.13",
    "reflect-metadata": "0.2.1",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "9.4.3",
    "@types/cron": "2.0.1",
    "@types/express": "4.17.21",
    "@types/express-session": "1.17.10",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.19",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "prisma": "4.16.2",
    "source-map-support": "0.5.21",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  },
  "jest": {
    "moduleNameMapper": {
      "~/(.*)$": "<rootDir>/../src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
